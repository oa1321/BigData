<!DOCTYPE html>
<html>
<head>
	<title>Redis Data Generator</title>
	<style>
		table {
		  border-collapse: collapse;
		  width: 100%;
		}
  
		th, td {
		  text-align: left;
		  padding: 8px;
		  border-bottom: 1px solid lightgray;
		}
  
		tr:nth-child(even) {background-color: #f2f2f2;}
  
		th {
		  background-color: #ef2400;
		  color: white;
		  border-bottom: 1px solid black;
		}
	  </style>
</head>
<body>
	<h1>Redis Data Generator</h1>
	<button onclick=gen()>Generate Data</button>
	<table>
		<tr>
		  <th>Orders Today</th>
		  <td><%= orders_today %></td>
		</tr>
		<tr>
		  <th>Open Orders</th>
		  <td><%= open_orders %></td>
		</tr>
		<tr>
		  <th>Open Stores</th>
		  <td><%= open_stores %></td>
		</tr>
		<tr>
		  <th>Average Time</th>
		  <td><%= avarage_time %></td>
		</tr>
		<tr>
		  <th>Last Update Time</th>
		  <td><%= update_time %></td>
		</tr>
		<tr>
		  <th>Olives</th>
		  <td><%= olives %></td>
		</tr>
		<tr>
		  <th>Corn</th>
		  <td><%= corn %></td>
		</tr>
		<tr>
		  <th>Mushrooms</th>
		  <td><%= mushrooms %></td>
		</tr>
		<tr>
		  <th>Anchovy</th>
		  <td><%= anshoby %></td>
		</tr>
		<tr>
		  <th>Onions</th>
		  <td><%= onions %></td>
		</tr>
		<tr>
		  <th>Cheese</th>
		  <td><%= cheese %></td>
		</tr>
		<tr>
		  <th>Peppers</th>
		  <td><%= peppers %></td>
		</tr>
		<tr>
		  <th>Tomato</th>
		  <td><%= tomato %></td>
		</tr>
		<tr>
		  <th>Cities Data</th>
		  <td>
			<table>
			  <tr>
				<th>City</th>
				<th>Orders</th>
			  </tr>
			  <% for (const [key, value] of Object.entries(cities_data)) { %>
			  <tr>
				<td><%= key %></td>
				<td><%= value %></td>
			  </tr>
			  <% } %>
			</table>
		  </td>
		</tr>
		<tr>
		  <th>Regions Data</th>
		  <td>
			<table>
			  <tr>
				<th>Region</th>
				<th>Orders</th>
			  </tr>
			  <% for (const [key, value] of Object.entries(regins_data)) { %>
			  <tr>
				<td><%= key %></td>
				<td><%= value %></td>
			  </tr>
			  <% } %>
			</table>
		  </td>
		</tr>
		<tr>
			<th>Times Data</th>
			<td>
			  <table>
				<tr>
				  <th>Time</th>
				  <th>Count</th>
				</tr>
				<% Object.entries(times_data).forEach(([time, count]) => { %>
				  <tr>
					<td><%= time %></td>
					<td><%= count %></td>
				  </tr>
				<% }); %>
			  </table>
			</td>
		  </tr>
	  </table>
	<script>
	async function gen(){
		window.location.reload();
	}
	
</script>
</body>

</html>